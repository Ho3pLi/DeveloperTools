{
  "manifest_version": 3,
  "name": "Developer Tools",
  "description": "trying to help",
  "version": "1.0",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "/assets/image.png"
  },
  "icons": {
    "16": "/assets/16x16.png",
    "32": "/assets/32x32.png",
    "48": "/assets/48x48.png",
    "128": "/assets/128x128.png"
  },
  "permissions": [
  "clipboardRead",
  "clipboardWrite"
  ]
}